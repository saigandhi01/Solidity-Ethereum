		"userdoc": {
			"methods": {
				"approve(address,uint256)": {
					"notice": "Set allowance for other address\r  \r Allows `_spender` to spend no more than `_value` tokens on your behalf\r  \r"
				},
				"approveAndCall(address,uint256,bytes)": {
					"notice": "Set allowance for other address and notify\r  \r Allows `_spender` to spend no more than `_value` tokens on your behalf, and then ping the contract about it\r  \r"
				},
				"burn(uint256)": {
					"notice": "Destroy tokens\r  \r Remove `_value` tokens from the system irreversibly\r  \r"
				},
				"burnFrom(address,uint256)": {
					"notice": "Destroy tokens from other account\r  \r Remove `_value` tokens from the system irreversibly on behalf of `_from`.\r  \r"
				},
				"transfer(address,uint256)": {
					"notice": "Transfer tokens\r  \r Send `_value` tokens to `_to` from your account\r  \r"
				},
				"transferFrom(address,address,uint256)": {
					"notice": "Transfer tokens from other address\r  \r Send `_value` tokens to `_to` on behalf of `_from`\r  \r"
				}
			}
